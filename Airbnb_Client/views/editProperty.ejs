<main id="site-content" role="main" ng-controller="editProperty_controller">


    <div id="notification-area"></div>
    <div class="page-container-responsive space-top-4 space-4">
        <div class="row">
            <div class="col-md-3 space-sm-4">
                <div class="sidenav">
                    <ul class="sidenav-list">
                        <li>
                            <img src="images/{{property.mediaId.imageUrl[0]}}" width="300px" height="200px">
                        </li>
                        <br/>
                        <li>
                            <img src="images/{{property.mediaId.imageUrl[1]}}" width="300px" height="200px">
                        </li>
                    </ul>

                </div>

            </div>
            <div class="col-md-1"></div>
            <div class="col-md-8">
                <div id="dashboard-content">
                    <form accept-charset="UTF-8">
                        <div class="panel row-space-4">
                            <div class="panel-header">
                                Property Details
                            </div>
                            <div class="panel-body">
                                <div class="row row-condensed space-4">
                                    <label class="text-right col-sm-3" for="user_first_name">
                                        Property Name
                                    </label>
                                    <div class="col-sm-9">
                                        <input id="user_first_name" size="30" class="focus" name="first_name"
                                               type="text" ng-model="property.name">
                                        <span class="text-danger"></span>
                                        <p style="color:red" ng-show="first_invalid">Property Name is Required</p>
                                    </div>
                                </div>

                                <div class="row row-condensed space-4">
                                    <label class="text-right col-sm-3" for="user_last_name">
                                        Property Description
                                    </label>
                                    <div class="col-sm-9">

                                        <input id="user_last_name" size="30" class="focus" name="last_name" type="text"
                                               ng-model="property.description">
                                        <span class="text-danger"></span>
                                        <p style="color:red" ng-show="last_invalid"> Property Description is
                                            Required</p>

                                    </div>
                                </div>


                                <div class="row row-condensed space-4">
                                    <label class="text-right col-sm-3" for="user_email">
                                        BedRooms
                                    </label>
                                    <div class="col-sm-9">

                                        <input id="user_email" size="30" class="focus" name="email" type="text"
                                               ng-model="property.bedrooms">

                                        <p style="color:red" ng-show="email_invalid">BedRooms is Required</p>

                                    </div>
                                </div>

                                <div class="row row-condensed space-4">
                                    <label class="text-right col-sm-3" for="user_live">
                                        BathRooms
                                    </label>
                                    <div class="col-sm-9">

                                        <input id="user_live" size="30" class="focus" name="live" type="text"
                                               ng-model="property.bathrooms">
                                        <p style="color:red" ng-show="address_invalid">BathRooms is Required</p>
                                    </div>
                                </div>

                                <div class="row row-condensed space-4">
                                    <label class="text-right col-sm-3" for="user_live">
                                        Price
                                    </label>
                                    <div class="col-sm-9">

                                        <input id="user_live" size="30" class="focus" name="live" type="text"
                                               ng-model="property.price">
                                        <p style="color:red" ng-show="State_invalid">Price is Required</p>
                                    </div>
                                </div>


                                <div class="row row-condensed space-4">
                                    <label class="text-right col-sm-3" for="user_live">
                                        Number of Beds
                                    </label>
                                    <div class="col-sm-9">

                                        <input id="user_live" size="30" class="focus" name="live" type="text"
                                               ng-model="property.beds">
                                        <p style="color:red" ng-show="city_invalid">Beds is Required</p>
                                    </div>
                                </div>


                            </div>
                        </div>


                        <button class="btn btn-primary btn-large" ng-click="saveProperty()">
                            Save
                        </button>


                        <div ng-show="success_model" class="alert alert-success">Property Details Saved Successfully.
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>

</main>

